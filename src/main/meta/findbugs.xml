<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="findbugsplugin.xsd" pluginid="com.syncron.findbugs" defaultenabled="true"
	provider="Syncron" website="http://www.syncron.com">

	<Detector class="com.syncron.findbugsextensions.PrintStackTraceDetector"
		reports="SYNC_PRINT_STACKTRACE" />
	<BugPattern type="SYNC_PRINT_STACKTRACE" abbrev="PST"
		category="CORRECTNESS" />
	<Detector class="com.syncron.findbugsextensions.FinalOverrideDetector"
		reports="SYNC_FINAL_OVERRIDDEN" />
	<BugPattern type="SYNC_FINAL_OVERRIDDEN" abbrev="AF" category="CORRECTNESS" />
</FindbugsPlugin>
